num1 = 12
num2 = 25
num3 = 21
num4 = 27
num5 = 8
num6 = 7
num7 = 13

result_a = num1 | num2
result_b = num3 & num4
result_c = num5 ^ num6
result_d = num7 & num6
result_e = num7 | num6

print("a. 12 | 25 =", result_a)
print("b. 21 & 27 =", result_b)
print("c. 8 ^ 7 =", result_c)
print("d. 13 & 7 =", result_d)
print("e. 13 | 7 =", result_e)
